{
    "manifest_version": 3,
    "name": "My Awesome Extension",
    "version": "1.0",
    "description": "A simple Chrome extension example.",
    "permissions": ["activeTab"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["grabber.js"]
    }
  ]
}